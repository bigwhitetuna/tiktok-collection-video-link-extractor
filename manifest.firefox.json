{
  "manifest_version": 2,
  "name": "TikTok Video Link Extractor",
  "version": "1.0.7",
  "description": "Manually extract TikTok video links after scrolling.",
  "permissions": [
    "activeTab",
    "tabs",
    "downloads"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon.png"
  }
}
